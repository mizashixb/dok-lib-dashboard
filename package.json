{
  "name": "dok-lib-dashboard",
  "version": "0.1.0",
  "private": false,
  "type": "module",
  "description": "A reusable Next.js dashboard components library",
  "license": "MIT",
  "author": "Il tuo Nome <tua.email@example.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuo-username/dok-lib-dashboard.git"
  },
  "keywords": [
    "nextjs",
    "dashboard",
    "react",
    "components",
    "ui"
  ],
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    },
    "./components": {
      "import": "./dist/components/index.js",
      "require": "./dist/components/index.js"
    }
  },
  "files": [
    "dist",
    "src/styles",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "clsx": "^2.1.0",
    "date-fns": "^3.6.0",
    "react-icons": "^5.2.1"
  },
  "peerDependencies": {
    "next": ">=14.0.0",
    "react": "^16.8 || ^17.0 || ^18.0 || ^19.0",
    "react-dom": "^16.8 || ^17.0 || ^18.0 || ^19.0",
    "tailwindcss": "^3.0.0 || ^4.0.0"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    },
    "tailwindcss": {
      "optional": true
    }
  },
  "devDependencies": {
    "typescript": "^5",
    "next": "15.2.4",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/jest": "^29.5.12",
    "tailwindcss": "^4.0.0",
    "@tailwindcss/postcss": "^4",
    "eslint": "^9",
    "eslint-config-next": "15.2.4",
    "jest": "^29.7.0",
    "@testing-library/react": "^15.0.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/user-event": "^14.5.1"
  }
}